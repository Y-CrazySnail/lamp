<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yeem.zero.mapper.ZeroProductMapper">
    <select id="selectByCategoryId" resultType="com.yeem.zero.entity.ZeroProduct">
        select zp.* from zero_product zp
        where zp.delete_flag = 0 and zp.category_id = #{categoryId}
        order by zp.sort
    </select>
</mapper>
