<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yeem.zero.mapper.ZeroProductImageMapper">
    <select id="selectByProductId" resultType="com.yeem.zero.entity.ZeroProductImage">
        select zpi.* from zero_product_image zpi
        where delete_flag = 0 and zpi.product_id = #{productId}
        order by zpi.sort
    </select>
</mapper>
