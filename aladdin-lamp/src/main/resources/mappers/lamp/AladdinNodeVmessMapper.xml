<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yeem.lamp.mapper.AladdinNodeVmessMapper">
    <update id="updateByValidServiceIdList">
        update aladdin_node_vmess
        set node_type = 'expired'
        where service_id not in
        <foreach item="serviceId" collection="serviceIdList" open="(" separator="," close=")">
            #{serviceId}
        </foreach>
    </update>
</mapper>